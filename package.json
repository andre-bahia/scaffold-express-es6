{
  "name": "scaffold-express-es6",
  "version": "1.0.0",
  "description": "Simple scaffold api with express to node js",
  "main": "dist",
  "scripts": {
    "dev": "nodemon -w src --exec \"babel-node src/app.js --presets es2015,stage-0\"",
    "build": "babel src -s -D -d dist --presets es2015,stage-0",
    "start": "node dist/app.js",
    "prestart": "npm run -s build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/andre-bahia/scaffold-express-es6.git"
  },
  "keywords": [
    "node.js",
    "javascript"
  ],
  "author": "Andr√© Luis",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/andre-bahia/scaffold-express-es6/issues"
  },
  "homepage": "https://github.com/andre-bahia/scaffold-express-es6#readme",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-preset-env": "^1.7.0",
    "babel-preset-stage-0": "^6.24.1",
    "nodemon": "^1.17.4"
  },
  "dependencies": {
    "express": "^4.16.3",
    "morgan": "^1.9.0"
  }
}
